<Window x:Class="DiscordBotMessageSenderUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DiscordBotMessageSenderUI"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Label x:Name="MainLabel" Content="Send a Message to a User..." HorizontalAlignment="Center" Height="52" Margin="0,45,0,0" VerticalAlignment="Top" Width="368" FontSize="26" FontWeight="Bold" />
        <Label x:Name="UserIdLabel" Content="User ID" HorizontalAlignment="Left" Height="52" Margin="238,124,0,0" VerticalAlignment="Top" Width="368" FontSize="20" FontWeight="Normal" />
        <TextBox x:Name="UserId" Margin="245,166,245,249" TextChanged="UserId_TextChanged">test</TextBox>
    </Grid>
</Window>
